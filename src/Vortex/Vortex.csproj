<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <Description>Lightweight Deep learning / Neural Netowkrs library</Description>
    <RepositoryUrl>https://github.com/void-intelligence/Vortex</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <PackageTags>Vortex, Deep Learning, Neural Network</PackageTags>
    <NeutralLanguage>en-US</NeutralLanguage>
    <PackageReleaseNotes>Initial Release</PackageReleaseNotes>
    <PackageProjectUrl>void-intelligence.github.io</PackageProjectUrl>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <Copyright>(C) Void-Intelligence Under Apache 2.0 License</Copyright>
    <Company>Void-Intelligence</Company>
    <Authors>Void-Intelligence</Authors>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Metric\Kernels\Regression\RSquared.cs~RF5f703330.TMP" />
    <None Remove="Optimizer\Kernels\AdaDelta.cs~RF12757067.TMP" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="VI-Nomad" Version="1.4.3" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Data\" />
  </ItemGroup>

